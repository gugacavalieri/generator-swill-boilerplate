{
  "base": [
    ".csslintrc",
    ".editorconfig",
    ".babelrc",
    ".eslintrc",
    ".gitattributes",
    ".gitignore",
    ".yo-rc.json",
    "app/favicon.ico",
    "app/fonts/.gitkeep",
    "app/img/copyright/.gitkeep",
    "app/img/logos/logo-monochrome-RGB.eps",
    "app/img/logos/logo-monochrome.eps",
    "app/img/logos/logo-monochrome.png",
    "app/img/logos/logo-monochrome.svg",
    "app/img/logos/logo-RGB.eps",
    "app/img/logos/logo.eps",
    "app/img/logos/logo.png",
    "app/img/logos/logo.svg",
    "config.json",
    "gulpfile.babel.js",
    "LICENSE",
    "package.json",
    "src/images/sprite/.gitkeep",
    "src/images/touch/apple-touch-icon.png",
    "src/images/touch/chrome-touch-icon-192x192.png",
    "src/scripts/index.js",
    "src/scripts/imports/javascript.js",
    "src/scripts/imports/javascript.test.js",
    "src/scripts/imports/google_analytics.js"
  ],

  "stylus": [
    "src/stylesheets/components/_index.styl",
    "src/stylesheets/helpers/_helpers.styl",
    "src/stylesheets/helpers/functions/_em.styl",
    "src/stylesheets/helpers/functions/_rem.styl",
    "src/stylesheets/helpers/mixins/_arrow.styl",
    "src/stylesheets/helpers/mixins/_circle.styl",
    "src/stylesheets/helpers/mixins/_clearfix.styl",
    "src/stylesheets/helpers/mixins/_opacity.styl",
    "src/stylesheets/helpers/mixins/_retina.styl",
    "src/stylesheets/partials/_form.styl",
    "src/stylesheets/partials/_html.styl",
    "src/stylesheets/partials/_index.styl",
    "src/stylesheets/partials/_print.styl",
    "src/stylesheets/partials/_text.styl",
    "src/stylesheets/settings/_variables.styl",
    "src/stylesheets/settings/_web-fonts.styl",
    "src/stylesheets/styles.styl"
  ],

  "sass": [
    "src/stylesheets/helpers/_helpers.sass",
    "src/stylesheets/helpers/_helpers.scss",
    "src/stylesheets/helpers/functions/_clean-unit.scss",
    "src/stylesheets/helpers/functions/_em.scss",
    "src/stylesheets/helpers/functions/_rem.scss",
    "src/stylesheets/helpers/mixins/_arrow.scss",
    "src/stylesheets/helpers/mixins/_clearfix.scss",
    "src/stylesheets/helpers/mixins/_retina.scss",
    "src/stylesheets/partials/_html.scss",
    "src/stylesheets/partials/_index.scss",
    "src/stylesheets/partials/_print.scss",
    "src/stylesheets/partials/_text.scss",
    "src/stylesheets/settings/_variables.scss",
    "src/stylesheets/settings/_web-fonts.scss",
    "src/stylesheets/styles.scss"
  ],

  "handlebars": {
    "enabled": {
      "all": [
        "src/handlebars/includes/footer.hbs",
        "src/handlebars/includes/header.hbs",
        "src/handlebars/index.html"
      ],
      "404": [
        "src/handlebars/404.html"
      ]
    },
    "disabled": {
      "all": [
        "app/index.html"
      ],
      "404": [
        "app/404.html"
      ]
    }
  },

  "optionals": [
    "README.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    ".travis.yml",
    ".npmignore",
    "app/.htaccess",
    "app/crossdomain.xml",
    "app/manifest.json",
    "app/browserconfig.xml",
    "app/robots.txt",
    "app/humans.txt",
    "src/images/touch/icon-128x128.png",
    "src/images/touch/ms-touch-icon-144x144-precomposed.png"
  ]
}